<h1>Admin::Leagues#edit</h1>
<div class="container">
<table>
<%= form_with model: @league, url: admin_league_path(@league.id), local:true do |f| %>
  <tr class="form-group">
    <th><label for="country">国・地域</label></th>
    <td><%= f.text_field :country %></td>
  </tr>
 
  <tr class="form-group">
    <th><label for="division">部</label></th>
    <td><%= f.text_field :division %>部</td>
  </tr>
 
  <tr class="form-group">
    <th><label for="name">リーグ名</label></th>
    <td><%= f.text_field :name %></td>
  </tr>
</table>
 
  <div class="actions">
    <%= f.submit '変更する', class: "btn btn-success" %>
  </div>
<% end %>
</div>